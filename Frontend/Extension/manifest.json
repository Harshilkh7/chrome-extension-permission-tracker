{
  "manifest_version": 3,
  "name": "My Vite React Extension",
  "version": "1.0",
  "description": "A Chrome extension built with Vite + React + Tailwind",
  
  "action": {
    "default_popup": "index.html"
 
  },



  "permissions": [
    "storage",
    "activeTab"
  ],

  "host_permissions": [
    "<all_urls>"
  ]


 
}
